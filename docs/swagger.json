            "type": "object",
            "additionalProperties": {}
        },
        "user_owner_internal_dto.CreateOrderRequest": {
            "type": "object",
            "required": [
                "city",
                "price",
                "region"
            ],
            "properties": {
                "city": {
                    "type": "string"
                },
                "price": {
                    "type": "number",
                    "minimum": 0
                },
                "region": {
                    "type": "string"
                }
            }
        },
        "user_owner_internal_dto.CreateUserReq": {
            "type": "object",
            "required": [
                "email",
                "name",
                "password",
                "phone_number"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "logo": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "phone_number": {
                    "type": "string"
                }
            }
        },
        "user_owner_internal_dto.LoginReq": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        }
    }
}